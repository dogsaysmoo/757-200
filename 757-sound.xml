        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>100.0</max-dist>
    </nosmoke>

    <!-- Overspeed warning -->
    <overspeed>
        <name>Overspeed warning</name>
        <path>Sounds/overspeed.wav</path>
        <mode>looped</mode>
        <condition>
            <greater-than>
                <property>velocities/airspeed-kt</property>
                <value>345</value>
            </greater-than>
        </condition>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
    </overspeed>
    <avhum>
        <path>Sounds/FL2070/757_inside.wav</path>
        <mode>looped</mode>
        <type>avionics</type>
		<name>avn</name>
		<condition>
			<property>/systems/electrical/outputs/efis</property>
		</condition>
        <volume>
			<property>sim/current-view/internal</property>
        </volume>
    </avhum>
	<avhum>
        <path>Sounds/FL2070/757_inside.wav</path>
        <mode>looped</mode>
        <type>avionics</type>
		<name>avn-off</name>
		<condition>
			<not><property>/systems/electrical/outputs/efis</property></not>
		</condition>
        <volume>
